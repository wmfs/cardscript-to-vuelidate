{
  "name": "@wmfs/cardscript-to-vuelidate",
  "description": "Takes in a Cardscript JSON object and converts it into a vuelidate object.",
  "version": "0.0.0-semantically-released",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "main": "./lib/index.js",
  "dependencies": {
    "@wmfs/json-to-vuelidate": "1.45.0",
    "@wmfs/cardscript-schema": "1.37.0",
    "dottie": "2.0.6"
  },
  "devDependencies": {
    "chai": "6.0.1",
    "codecov": "3.8.3",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "11.7.2",
    "nyc": "17.1.0",
    "semantic-release": "24.2.8",
    "standard": "17.1.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@semantic-release/git": "10.0.1"
  },
  "scripts": {
    "lint": "standard",
    "testold": "mocha --require babel-register",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/cardscript-to-vuelidate.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/cardscript-to-vuelidate/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
